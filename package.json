{
  "name": "code-traverse",
  "author": "Ferdinand Antonius",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/code-traverse.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "ts-node -T -O '{\"module\": \"commonjs\"}' src/run.ts",
    "start": "tsdx watch",
    "build": "tsdx build --transpileOnly",
    "test": "jest",
    "lint": "tsdx lint",
    "tsc": "tsc",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@babel/parser": "^7.18.13",
    "@babel/traverse": "^7.18.13",
    "glob-to-regexp": "^0.4.1",
    "globby": "^11.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.18.13",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "husky": "^8.0.1",
    "jest": "^29.0.0",
    "size-limit": "^8.0.1",
    "ts-jest": "^28.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {},
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/code-traverse.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/code-traverse.esm.js",
      "limit": "10 KB"
    }
  ]
}
